{
  "cli": {
    "version": ">= 3.19.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"
    },
    "production": {
      "ios": {
        "simulator": false,
        "autoIncrement": true
      },
      "android": {
        "gradleCommand": ":app:bundleRelease"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://mygraceguide.app/"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
